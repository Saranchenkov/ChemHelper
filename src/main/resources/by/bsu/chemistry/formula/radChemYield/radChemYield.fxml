            <font>
                <Font name="Times New Roman" size="24.0" />
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </Label>
        <ImageView fitHeight="87.0" fitWidth="291.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../pictures/radChemYield.png" />
            </image>
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </ImageView>
        <GridPane alignment="CENTER" prefHeight="222.0" prefWidth="585.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="188.0" minWidth="99.0" prefWidth="181.0" />
